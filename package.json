{
  "name": "@roberp05/package-publishing",
  "version": "1.1.8",
  "main": "index.js",
  "scripts": {
    "build": "echo 'Building your project... (replace with your actual build command)'",
    "npm:publish:patch": "node ./publish/npm.js patch",
    "npm:publish:minor": "node ./publish/npm.js minor",
    "npm:publish:major": "node ./publish/npm.js major",
    "npm:publish:patch:with-push": "node ./publish/npm.js patch --push",
    "npm:publish:minor:with-push": "node ./publish/npm.js minor --push",
    "npm:publish:major:with-push": "node ./publish/npm.js major --push",
    "github:publish:patch": "node ./publish/github.js patch",
    "github:publish:minor": "node ./publish/github.js minor",
    "github:publish:major": "node ./publish/github.js major",
    "github:publish:patch:with-push": "node ./publish/github.js patch --push",
    "github:publish:minor:with-push": "node ./publish/github.js minor --push",
    "github:publish:major:with-push": "node ./publish/github.js major --push"
  },
